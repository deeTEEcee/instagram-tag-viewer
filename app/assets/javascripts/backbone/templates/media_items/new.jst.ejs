<h1>New media_item</h1>

<form id="new-media_item" name="media_item">
  <div class="field">
    <label for="type"> type:</label>
    <input type="text" name="type" id="type" value="<%= type %>" >
  </div>

  <div class="field">
    <label for="tag"> tag:</label>
    <input type="text" name="tag" id="tag" value="<%= tag %>" >
  </div>

  <div class="field">
    <label for="link"> link:</label>
    <input type="text" name="link" id="link" value="<%= link %>" >
  </div>

  <div class="field">
    <label for="image"> image:</label>
    <input type="text" name="image" id="image" value="<%= image %>" >
  </div>

  <div class="field">
    <label for="username"> username:</label>
    <input type="text" name="username" id="username" value="<%= username %>" >
  </div>

  <div class="field">
    <label for="tagged_at"> tagged_at:</label>
    <input type="text" name="tagged_at" id="tagged_at" value="<%= tagged_at %>" >
  </div>

  <div class="actions">
    <input type="submit" value="Create Media item" />
  </div>

</form>

